{
  "printWidth": 100,
  "singleQuote": true,
  "plugins": ["prettier-plugin-organize-imports"],
  "overrides": [
    {
      "files": ["*.json", "*.json5"],
      "options": {
        "printWidth": 80,
        "trailingComma": "none"
      }
    },
    {
      "files": ["*.md"],
      "options": {
        "proseWrap": "always"
      }
    }
  ]
}
